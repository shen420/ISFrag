## The function is currently defined as
function (ISF_List, featureID, directory)
{
    write.csv(ISF_List[[which(grepl(featureID, names(ISF_List),
        fixed = TRUE) == T)]], file = paste0(featureID, "_Detailed.csv"),
        row.names = T, col.names = T)
  }
}
